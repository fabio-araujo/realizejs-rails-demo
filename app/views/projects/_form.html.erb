<%
  form_props = {
    resource: 'project',
    data: @project,
    method: locals[:httpMethod],
    action: locals[:action],
    otherButtons: locals[:otherButtons],
    inputs: {
      name: {
        label: 'Name'
      }
    }
  }
%>

<%=
  react_component 'ProjectsForm', {
    form: form_props,
    title: locals[:title]
  }
%>